{
  "name": "tech-shop",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "eslint --color",
      "prettier --check"
    ],
    "src/**/*.scss": [
      "stylelint",
      "prettier --check"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "antd": "^5.10.0",
    "axios": "^1.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "react-query": "^3.39.3",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "typescript": "4.6.4",
    "typewriter-effect": "^2.21.0",
    "web-vitals": "^2.1.0",
    "zustand": "^4.4.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "run-s lint:ts prettier-write prettier-write:ts prettier-write:style",
    "lint:ts": "eslint ./src --ext .ts --ext .tsx --ext .js --fix",
    "lint:style": "stylelint \"./src/**/*.{css,scss}\"",
    "lint:type": "tsc -p ./tsconfig.json",
    "prettier-check": "prettier --check \"src/**/*.@(ts|tsx|scss)\"",
    "prettier-check:script": "prettier --check \"src/**/*.@(ts|tsx)\"",
    "prettier-check:style": "prettier --check \"src/**/*.@(scss)\"",
    "prettier-write": "prettier --write \"src/**/*.@(ts|tsx|scss)\"",
    "prettier-write:ts": "prettier --write \"*.@(ts|tsx|js|jsx|scss)\"",
    "prettier-write:script": "prettier --write \"src/**/*.@(ts|tsx)\"",
    "prettier-write:style": "prettier --write \"src/**/*.@(scss)\"",
    "gen:component": "hygen component new"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/optimize-css-assets-webpack-plugin": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.17.0",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.7",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.1",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.49.9",
    "sass-resources-loader": "^2.2.4",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^5.0.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.70.0"
  }
}
